  <script>
    // Utils functions
    function format(str, ...values) {
      return str.replace(/{(\d+)}/g, function (match, index) {
        return typeof values[index] !== 'undefined' ? values[index] : match;
      });
    }

    // Event based functions
    function nice() {
      let wallpapers = ["assets/1390156669.gif", "assets/wallpaper1.jpg"]
      cwallpaper = document.getElementsByTagName("body")[0].style.background
      if (cwallpaper.includes(wallpapers[1])) {
        document.getElementsByTagName("body")[0].style.background = format('url({0})', wallpapers[0]);
      } else {
        document.getElementsByTagName("body")[0].style.background = format('url({0})', wallpapers[1]);
      }

      document.getElementsByTagName("body")[0].style.backgroundRepeat = 'no-repeat'
      document.getElementsByTagName("body")[0].style.backgroundSize = 'cover'
    }

